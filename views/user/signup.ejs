<%-include('head.ejs')%>

<%-include('../navbar.ejs')%>
<br>
<div class="container">
    <h2>Compte</h2>
    <br>
   
    <%for(let i of messageserror ){ %>
        <div class="alert alert-danger">
            <%=i%>
        </div>
        <%}%>
    <form action="/users/signup" method="post">
<div class="form-group">
    <label>Email </label><br>
    <input type="email" name="email" placeholder="Entrer email" class="form-control">
</div>
<div class="form-group">
    <label>Mot de passe </label><br>
    <input type="password" name="password" placeholder="Entrer mot de passe " class="form-control">
</div>
<div class="form-group">
    <label>Confirmer mot de passe </label><br>
    <input type="password" name="cpassword" placeholder="Confirmer mot de passe" class="form-control">
</div>
<input type="hidden" name="_csrf" value="<%=token%>"/>
<input type="submit" class="btn btn-primary" value="inscrire">
</form>
Vous avez deja un compte ? <a href="/users/signin"> Se connecter</a>
</div>